{
  "name": "books-library",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "autoprefixer": "7.1.6",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "google-books-search": "^0.3.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "^22.4.3",
    "mobx": "^3.5.1",
    "mobx-react": "^4.4.1",
    "morgan": "^1.9.0",
    "normalize.css": "^8.0.0",
    "object-assign": "4.1.1",
    "postcss-css-variables": "^0.8.0",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-hexrgba": "^1.0.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "2.0.8",
    "postcss-nested": "^3.0.0",
    "promise": "8.0.1",
    "prop-types": "^15.6.0",
    "query-string": "^5.1.0",
    "raf": "3.4.0",
    "react": "^16.2.0",
    "react-dev-utils": "^5.0.0",
    "react-dom": "^16.2.0",
    "react-icons": "^2.2.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-svg-spinner": "^0.2.0",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "throttle-debounce": "^1.0.1",
    "ts-jest": "^22.4.2",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "PORT=9000 concurrently \"npm run server\" \"npm run client\"",
    "client": "NODE_PATH=src node scripts/start.js",
    "server": "node server",
    "build": "NODE_PATH=src node scripts/build.js",
    "test": "NODE_PATH=src node scripts/test.js --env=jsdom --no-cache"
  },
  "proxy": "http://localhost:9001/",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx,js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/config/jest/test-shim.js",
      "<rootDir>/config/jest/test-setup.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{ts|tsx|js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{ts|tsx|js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^.+\\.(js|jsx)$": "<rootDir>/config/jest/test-preprocessor.js",
      "^.+\\.(ts|tsx)?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(ts|tsx|js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.9",
    "@types/jest": "^22.2.2",
    "@types/react": "^16.3.5",
    "@types/react-dom": "^16.0.4",
    "@types/react-icons": "^2.2.5",
    "@types/react-router": "^4.0.23",
    "@types/react-router-dom": "^4.2.6",
    "awesome-typescript-loader": "^4.0.0",
    "concurrently": "^3.5.1",
    "jest-localstorage-mock": "^2.2.0",
    "react-test-renderer": "^16.3.1",
    "source-map-loader": "^0.2.3",
    "typescript": "^2.4.0"
  }
}
